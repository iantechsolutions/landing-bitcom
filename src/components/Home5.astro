.slider {
    width: 60vw;
    height: auto;
    margin: auto;
    overflow: hidden;
    background-image: linear-gradient(250deg, #1bdfb7 0%, #abd6d0 100%);
    border-radius: 15px;
    padding: 3vw;
}

.slider .slide-track {
    display: flex;
    animation: scroll 40s linear infinite;
    -webkit-animation: scroll 40s linear infinite;
    width: calc(11vw * 14);
    gap: 1.3vw;
}

.slider .slide {
    /* width: 200px; */
}

.slider .slide img {
    width: 100%;
}

@keyframes scroll {
    0% {
        -webkit-transform: translateX(0);
        transform: translateX(0);
    }
    100% {
        -webkit-transform: translateX(calc(-11vw * 7));
        transform: translateX(calc(-11vw * 7));
    }
}


